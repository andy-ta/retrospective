{
  "name": "retrospective",
  "version": "0.2.1",
  "description": "Collaborative sticky notes for sprint retrospectives.",
  "license": "MIT",
  "author": "andy-ta",
  "contributors": [
    "RiadGahlouz"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:server": "npx @fluidframework/azure-local-service@latest",
    "start:azure": "cross-env REACT_APP_FLUID_CLIENT='\"azure\"' npm run start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-restricted-globals": [
        "error",
        "event",
        "fdescribe"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fluentui/react": "^8.34.2",
    "@fluidframework/azure-client": "^0.48.1",
    "@fluidframework/test-client-utils": "^0.48.1",
    "cross-env": "^7.0.3",
    "fluid-framework": "^0.48.1",
    "react": "^16.10.2",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^16.10.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "22.2.3",
    "@types/node": "^12.19.0",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/uuid": "^8.3.0",
    "react-scripts": "4.0.2",
    "typescript": "~4.4.3"
  },
  "private": true,
  "packageManager": "yarn@3.1.1"
}
